{
    "env": {
        "windows": {
            "VENV_MODE": true,
            "QEMU_IMG_DIR": "D:\\repository\\env\\tools\\qemu",
            "QEMU_DIR": "D:\\RT-ThreadStudio\\repo\\Extract\\Debugger_Support_Packages\\RealThread\\QEMU\\4.2.0.4",
            "PATH": "%PATH%;%QEMU_IMG_DIR%;%QEMU_DIR%"
        },
        "linux": {},
        "common": {}
    },
    "statusBarItem": {
        "build": {
            "icon": "$(zap)",
            "enable": true,
            "commands": [
                "scons -j12"
            ],
            "label": "编译",
            "tooltip": "编译 RT-Thread 内核"
        },
        "clean": {
            "icon": "$(clear-all)",
            "enable": true,
            "commands": [
                "scons -c"
            ],
            "label": "清理",
            "tooltip": "清理 RT-Thread 内核"
        },
        "qemu-run": {
            "enable": true,
            "commands": [
                "qemu.bat"
            ],
            "label": "运行",
            "tooltip": "使用 QEMU 模拟器运行",
            "icon": "$(run)"
        },
        "build-s": {
            "enable": true,
            "commands": [
                "scons"
            ],
            "label": "编译-单线程",
            "tooltip": "编译 RT-Thread 内核",
            "icon": "$(zap)"
        }
    },
    "home": {
        "show": true
    }
}